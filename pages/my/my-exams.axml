<view class='page-container-classname'>
                <view class='page' ref='saveChildRef0'>
  <view class='page__hd weui-panel__compact'>
    <view class='weui-panel' a:for='{{listData}}' a:key='*this' ref-numbers='{{listData}}'>
      <view class='weui-panel__bd'>
        <navigator url='../exam/exam-result?id={{item[0]}}' class='weui-media-box rktk-record' hover-class='weui-cell_active'>
          <view class='weui-media-box__title'>
            {{item[5]}}{{item[1]}}
          </view>
          <view class='weui-media-box__info'>
            <view class='weui-media-box__info__meta'>
              得分              <text>
                {{item[4]}}
              </text>
            </view>
            <view class='weui-media-box__info__meta weui-media-box__info__meta_extra'>
              耗时              <text>
                {{item[3]}}
              </text>
            </view>
            <view class='weui-media-box__info__meta weui-media-box__info__meta_extra'>
              {{item[2]}}
            </view>
          </view>
        </navigator>
      </view>
    </view>
  </view>
  <view class='page__bd'>
    <view class='icon-box' hidden='{{!nodata}}'>
      <icon type='info' size='{{93}}' class=' iconinfo-style 93-style'>
      </icon>
      <view class='icon-box__title'>
        暂无答题
      </view>
    </view>
  </view>
</view>
            </view>